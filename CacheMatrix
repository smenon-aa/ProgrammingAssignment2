
## Part 1 - Make the cache matrix
    
## set: Input matrix 
## get: the matrix
## setinverse: obtains inverse of the matrix 
## getinverse: gets inverse     
    
makeCacheMatrix <- function(x = matrix()) {
    inv <- NULL
    set <- function(y) {
        x <<- y
        inv <<- NULL
    }
    get <- function() x
    setinverse <- function(inverse) inv <<- inverse
    getinverse <- function() inv
    list(set = set, get = get,
         setinverse = setinverse,
         getinverse = getinverse)
}

## Part 2 - solve the cache matrix 
# Matrix Inverse not in cache - so have to compute it fresh

cachesolve <- function(x, ...) {
    inv <- x$getinverse()
    if(!is.null(inv)) {
        message("getting cached data")
        return(inv)
    }
    data <- x$get()
    inv <- solve(data, ...)
    x$setinverse(inv)
    inv
}
